<% provide(:page_title, "Demo") %>

<% content_for :head do %>
    <%= render "casham/modules/head", page_title: "Two Column Template" %>
<% end %>

<% content_for :header do %>
    <%#= render "casham/modules/header" %>
<% end %>

<% content_for :hero do %>
    <%= render "casham/modules/landing_hero", cols: "eight", headline_text: "Two Column Content Headline" %>
<% end %>


<% content_for :main_content do %>

    <div class="eight columns push-four main-body" role="content"> <!-- Main Content -->

        <%= render "casham/modules/widget_service_spotlight" %>
        <%= render "casham/modules/module_article" %>
        <%= render "casham/modules/module_store_locator" %>

    </div><!-- End Main Content -->



        <!-- Sidebar -->
    <aside class="four columns pull-eight side-bar">
        <h2 class="semantic-only">Related Content</h2>
      <div class="side-bar-container">


        <%= render "casham/modules/module_store_locator" %>
<%= render "casham/modules/widget_service_spotlight" %>

      </div>
    </aside> <!-- End Sidebar -->




<% end %>

<% content_for :footer do %>
    <%= render "casham/modules/footer" %>
<% end %>